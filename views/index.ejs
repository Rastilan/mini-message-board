<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(function(message) { %>
        <li>
          <strong><%= message.user %></strong>: <%= message.text %>
          <span><%= message.added.toISOString() %></span>
        </li>
      <% }) %>
    </ul>
  </body>
</html>
